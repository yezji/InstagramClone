<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.green.instagramclone">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:name=".config.ApplicationClass"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.InstagramClone"
        android:usesCleartextTraffic="true"
        tools:ignore="AllowBackup">
        <activity android:name=".src.main.home.createfeedorstory.feed.createfeed.CreateFeedActivity"></activity>
        <activity android:name=".src.register.registerfinal.RegisterFinalActivity" />
        <activity android:name=".src.register.addprofile.RegisterProfileActivity" />
        <activity android:name=".src.register.scope.RegisterOpenScopeActivity" />
        <activity android:name=".src.register.editnickname.RegisterEditNicknameActivity" />
        <activity android:name=".src.register.usenickname.RegisterConfirmNicknameActivity" />
        <activity android:name=".src.register.terms.RegisterTermsActivity" />
        <activity android:name=".src.register.birthday.RegisterBirthdayActivity" />
        <activity android:name=".src.register.createnickname.RegisterNicknameActivity" />
        <activity android:name=".src.register.authemailcode.AuthEmailActivity" />
        <activity android:name=".src.register.createid.RegisterIdActivity" />
        <activity android:name=".src.login.LoginActivity" />
        <activity android:name=".src.splash.SplashActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".src.main.MainActivity">

        </activity>
    </application>

</manifest>